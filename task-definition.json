{
    "family": "first-run-task-definition",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "ecs-jenkins",
            "image": "933929260845.dkr.ecr.us-east-2.amazonaws.com/jenkis-pipeline:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}